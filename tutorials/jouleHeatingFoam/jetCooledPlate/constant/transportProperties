FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Electrical resistivity at reference temperature (Ohm-meter)
rho0              rho0  [1 3 -3 0 0 -2 0] 6.90e-7;
// Temperature coefficient for linear approximation
alpha             alpha [0 0 0 -1 0 0 0]  0.00094;
/*alpha             alpha [0 0 0 -1 0 0 0]  1;*/
// Reference temperature for linear approximation
T0                T0    [0 0 0 1 0 0 0]   300;



